{
  "name": "angular-5-data-table-bootstrap-4",
  "version": "0.0.1",
  "description": "An Angular 5 data table, with pagination, sorting, expandable rows etc. Bootstrap 4 version.",
  "keywords": [
    "angular",
    "angular5",
    "Angular 5",
    "ng2",
    "datatable",
    "data-table",
    "data table",
    "Bootstrap",
    "Bootstrap 4",
    "pagination"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/datatable.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/datatable.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:softdesignconsultoria/angular-5-data-table-bootstrap-4.git"
  },
  "peerDependencies": {
    "@angular/core": "^4.0.1"
  },
  "author": "softdesignconsultoria <ernani.ely@softdesign.com.br>",
  "license": "MIT",
  "devDependencies": {
    "@angular/compiler": "^5.0.0",
    "@angular/compiler-cli": "^5.0.0",
    "rollup": "^0.50.0",
    "typescript": "^2.5.3",
    "uglify-js": "^3.1.3"
  },
  "dependencies": {
    "@angular/common": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@types/node": "~8.0.33",
    "rxjs": "^5.1.0",
    "ts-node": "~3.3.0",
    "zone.js": "^0.8.12"
  },
  "false": {},
  "bugs": {
    "url": "https://github.com/softdesignconsultoria/angular-5-data-table-bootstrap-4/issues"
